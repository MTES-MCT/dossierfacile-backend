<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.10.xsd">
    <changeSet id="202503201700" author="Nicolas">
        <!-- Ajout des colonnes dans la table tenant -->
        <addColumn tableName="tenant">
            <column name="tenant_first_name" type="varchar(255)"/>
            <column name="tenant_last_name" type="varchar(255)"/>
            <column name="tenant_preferred_name" type="varchar(255)"/>
            <column name="owner_type" type="varchar(50)"/>
        </addColumn>

    </changeSet>
</databaseChangeLog>
